            });
            r.addEventListener("click", () => {
              console.log("üñ±Ô∏è Category clicked:", r.value);
              
              // Desmarcar outras categorias
              categoryRadios.forEach(otherRadio => {
                if (otherRadio !== r) {
                  otherRadio.checked = false;
                  const otherLabel = otherRadio.closest('label');
                  if (otherLabel) {
                    otherLabel.classList.remove('active', 'btn-secondary');
                    otherLabel.classList.add('btn-outline-secondary');
                  }
                }
              });
              
              // Marcar o selecionado
              const label = r.closest('label');
              if (label) {
                label.classList.add('active', 'btn-secondary');
                label.classList.remove('btn-outline-secondary');
              }
              
              setTimeout(onChoiceChange, 100);
            });
            
            // Teste adicional - listener no label
            const label = r.closest('label');
            if (label) {
              label.addEventListener("click", () => {
                console.log("üñ±Ô∏è Category label clicked:", r.value);
                setTimeout(() => {
                  // Desmarcar outras categorias
                  categoryRadios.forEach(otherRadio => {
                    if (otherRadio !== r) {
                      otherRadio.checked = false;
                      const otherLabel = otherRadio.closest('label');
                      if (otherLabel) {
                        otherLabel.classList.remove('active', 'btn-secondary');
                        otherLabel.classList.add('btn-outline-secondary');
                      }
                    }
                  });
                  
                  // Marcar o selecionado
                  r.checked = true;
                  label.classList.add('active', 'btn-secondary');
                  label.classList.remove('btn-outline-secondary');
                  
                  setTimeout(onChoiceChange, 100);
                }, 50);
              });
            }
          });

          // Observer para detectar mudan√ßas nos inputs
          const observer = new MutationObserver((mutations) => {
            mutations.forEach((mutation) => {
              if (mutation.type === 'attributes' && mutation.attributeName === 'checked') {
                console.log("üëÅÔ∏è MutationObserver detectou mudan√ßa");
                setTimeout(onChoiceChange, 100);
              }
            });
          });

          // Observa mudan√ßas em todos os inputs
          [...genderRadios, ...categoryRadios].forEach(input => {
            observer.observe(input, { attributes: true, attributeFilter: ['checked'] });
          });
          
          console.log("‚úÖ Event listeners configurados com sucesso!");
        }

        // Adiciona os listeners
        addChoiceListeners();

        // Bot√£o confirmar
        btnConfirm.addEventListener("click", () => {
          if (currentChoice) {
            console.log("‚úÖ Confirmando escolha:", currentChoice);
            
            // Remove a anima√ß√£o
