          }
          if (cidadeSelect) {
            cidadeSelect.classList.remove('success');
            cidadeSelect.classList.add('error');
          }
          
          // 4. Esconde a se√ß√£o de confirma√ß√£o
          confirmationSection.style.display = 'none';
          confirmationSection.style.visibility = 'hidden';
          confirmationSection.style.opacity = '0';
          confirmationSection.style.animation = 'none';
          
          // 5. Reseta o estado
          currentChoice = null;
          isConfirmed = false;
          pendingChange = null;
          
          // 6. For√ßa atualiza√ß√£o visual dos bot√µes
          setTimeout(() => {
            // For√ßa re-render dos bot√µes
            genderRadios.forEach(r => {
