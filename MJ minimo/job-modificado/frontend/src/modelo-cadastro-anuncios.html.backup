      /* Estilos para os bot√µes de servi√ßos */
      .btn-group.services {
        gap: 8px;
        margin-bottom: 10px;
      }

      .btn-group.services .btn {
        border: 2px solid #e0e0e0;
        background: #fff;
        color: #333;
        padding: 8px 12px;
        margin: 2px;
        border-radius: 20px;
        font-size: 12px;
        transition: all 0.3s ease;
        cursor: pointer;
        display: inline-flex;
        align-items: center;
        justify-content: center;
        min-width: auto;
        white-space: nowrap;
      }

      .btn-group.services .btn:hover {
        border-color: #007bff;
        background: #f8f9fa;
      }

      .btn-group.services .btn input[type="checkbox"] {
        display: none;
