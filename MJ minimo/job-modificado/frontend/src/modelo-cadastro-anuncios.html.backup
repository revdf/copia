                label.classList.add('btn-outline-secondary');
              }
            });
            
            console.log("üßπ Todos os radio buttons foram desmarcados");
            
            // For√ßa um pequeno delay para garantir que o DOM foi atualizado
            setTimeout(() => {
              // Marca APENAS as novas op√ß√µes
              const newGenderRadio = document.querySelector(`input[name="category"][value="${pendingChange.gender}"]`);
              const newCategoryRadio = document.querySelector(`input[name="categoria"][value="${pendingChange.category}"]`);
              
              if (newGenderRadio) {
                newGenderRadio.checked = true;
                const label = newGenderRadio.closest('label');
                if (label) {
                  label.classList.add('active', 'btn-secondary');
                  label.classList.remove('btn-outline-secondary');
                }
                console.log(`‚úÖ Novo g√™nero marcado: ${pendingChange.gender}`);
              }
              
              if (newCategoryRadio) {
                newCategoryRadio.checked = true;
                const label = newCategoryRadio.closest('label');
                if (label) {
                  label.classList.add('active', 'btn-secondary');
                  label.classList.remove('btn-outline-secondary');
