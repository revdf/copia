      /* Efeitos visuais para se√ß√µes inativas */
      .form-section[style*="opacity: 0.5"] {
        pointer-events: none;
        filter: grayscale(70%);
        transition: all 0.3s ease;
      }

      /* Se√ß√µes bloqueadas - CSS mais forte */
      .form-section.blocked {
        display: none !important;
        visibility: hidden !important;
        opacity: 0 !important;
        height: 0 !important;
        overflow: hidden !important;
        margin: 0 !important;
        padding: 0 !important;
      }
      
      /* For√ßa o bloqueio inicial de todas as se√ß√µes exceto 1 e 2 */
      .form-section:not(#step-1):not(#step-2) {
        display: none !important;
      }

      /* Se√ß√µes desbloqueadas */
      .form-section.unlocked {
        display: block !important;
        animation: fadeIn 0.5s ease-in;
        margin: 0 auto 30px auto !important;
        padding: 20px !important;
        background: #f8f9fa !important;
        border-radius: 8px !important;
        border: 1px solid #e9ecef !important;
        width: 72% !important;
        max-width: 900px !important;
        clear: both !important;
        position: relative !important;
        z-index: 1 !important;
        visibility: visible !important;
        opacity: 1 !important;
        height: auto !important;
        overflow: visible !important;
      }

      @keyframes fadeIn {
        from { opacity: 0; transform: translateY(20px); }
        to { opacity: 1; transform: translateY(0); }
      }

      @keyframes pulse {
        0% { transform: scale(1); box-shadow: 0 4px 15px rgba(40, 167, 69, 0.3); }
        50% { transform: scale(1.02); box-shadow: 0 6px 20px rgba(40, 167, 69, 0.5); }
        100% { transform: scale(1); box-shadow: 0 4px 15px rgba(40, 167, 69, 0.3); }
      }

