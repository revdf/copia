            step1.scrollIntoView({ behavior: 'smooth', block: 'start' });
          }
        });

        // Bot√£o confirmar mudan√ßa
        btnConfirmChange.addEventListener("click", () => {
          if (pendingChange) {
            currentChoice = pendingChange;
            unlockSpecificSections(pendingChange.gender, pendingChange.category);
            changeConfirmationSection.style.display = 'none';
            confirmationSection.style.display = 'none';
            isConfirmed = true;
            pendingChange = null;
            console.log("‚úÖ Mudan√ßa confirmada:", currentChoice);
          }
        });

        // Bot√£o cancelar mudan√ßa
        btnCancelChange.addEventListener("click", () => {
          // Volta para a escolha anterior
          if (currentChoice) {
            const genderInput = document.querySelector(`input[name="category"][value="${currentChoice.gender}"]`);
            const categoryInput = document.querySelector(`input[name="categoria"][value="${currentChoice.category}"]`);
            
