        // Fun√ß√£o para desbloquear se√ß√µes espec√≠ficas
        function unlockSpecificSections(gender, category) {
          console.log(`üîì Desbloqueando se√ß√µes para: ${gender} + ${category}`);
          
          // Primeiro bloqueia todas
          sectionsToBlock.forEach(sec => {
            sec.style.display = 'none';
            sec.classList.add('blocked');
            sec.classList.remove('unlocked');
          });

          // Define quais se√ß√µes desbloquear baseado na escolha
          const sectionsToUnlock = [];

          if (category === "acompanhantes") {
            if (gender === "mulher") {
              // Mulher acompanhante - s√≥ se√ß√µes b√°sicas
            } else if (gender === "trans") {
              sectionsToUnlock.push("trans");
            } else if (gender === "homem") {
              sectionsToUnlock.push("homem", "euFaco");
            } else if (gender === "mulher-luxo") {
              // Mulher luxo - s√≥ se√ß√µes b√°sicas
            }
          } else if (category === "massagistas") {
            if (gender === "mulher") {
              sectionsToUnlock.push("massagista");
            } else if (gender === "mulher-luxo") {
              sectionsToUnlock.push("massagista");
            } else if (gender === "trans") {
              sectionsToUnlock.push("trans", "massagista");
            } else if (gender === "homem") {
              sectionsToUnlock.push("homem", "massagista");
            }
          } else if (category === "sexo-virtual") {
            if (gender === "mulher") {
              sectionsToUnlock.push("sexoVirtual");
            } else if (gender === "trans") {
              sectionsToUnlock.push("trans", "sexoVirtual");
            } else if (gender === "homem") {
              sectionsToUnlock.push("homem", "sexoVirtual");
            } else if (gender === "mulher-luxo") {
              sectionsToUnlock.push("sexoVirtual");
            }
          }

          // Desbloqueia as se√ß√µes espec√≠ficas
          sectionsToUnlock.forEach(key => {
            if (sections[key]) {
              sections[key].style.setProperty('display', 'block', 'important');
              sections[key].style.setProperty('visibility', 'visible', 'important');
              sections[key].style.setProperty('opacity', '1', 'important');
              sections[key].classList.remove('blocked');
              sections[key].classList.add('unlocked');
              console.log(`üîì Se√ß√£o desbloqueada: ${sections[key].id}`);
            }
          });

          // Desbloqueia sempre as se√ß√µes b√°sicas (step-8 em diante)
          for (let i = 8; i <= 13; i++) {
            const sec = document.getElementById(`step-${i}`);
            if (sec) {
              sec.style.setProperty('display', 'block', 'important');
              sec.style.setProperty('visibility', 'visible', 'important');
              sec.style.setProperty('opacity', '1', 'important');
              sec.classList.remove('blocked');
              sec.classList.add('unlocked');
              
              // Remove o atributo required de campos ocultos para evitar erros de valida√ß√£o
              const requiredFields = sec.querySelectorAll('input[required], select[required], textarea[required]');
