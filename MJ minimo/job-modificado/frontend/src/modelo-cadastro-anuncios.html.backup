          }
          
          if (cidadeSelect) {
            cidadeSelect.innerHTML = '<option value="">Selecione uma cidade</option>';
            cidadeSelect.selectedIndex = 0;
            cidadeSelect.value = '';
            console.log("üóëÔ∏è Limpando sele√ß√£o de cidade");
          }
          
          if (bairroInput) {
            bairroInput.value = '';
            console.log("üóëÔ∏è Limpando campo de bairro");
          }
          
          // 3. Remove classes de sucesso dos selects
          if (estadoSelect) {
            estadoSelect.classList.remove('success');
