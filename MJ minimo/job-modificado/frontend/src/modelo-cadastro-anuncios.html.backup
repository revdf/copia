      // JavaScript para upload de fotos
      function setupPhotoUpload(inputId, previewId, uploadBoxId) {
        const input = document.getElementById(inputId);
        const preview = document.getElementById(previewId);
        const uploadBox = document.getElementById(uploadBoxId);
        const uploadText = uploadBox.querySelector(".upload-text");

        // Fun√ß√£o para processar o arquivo selecionado
        function processFile(file) {
          if (!file) return;

          // Validar tipo de arquivo
          if (!file.type.startsWith("image/")) {
            alert("Por favor, selecione apenas arquivos de imagem.");
            input.value = ""; // Limpar o input
            return;
          }

          // Limites premium baseados na an√°lise dos sites de luxo
          const limits = {
            capa: 400 * 1024, // 400KB - Foto principal (qualidade premium)
            stories: 350 * 1024, // 350KB - Foto secund√°ria (alta qualidade)
            galeria: 300 * 1024, // 300KB - Galeria (boa qualidade)
          };

          // Determinar tipo de foto baseado no ID
          let photoType = "galeria"; // padr√£o
          if (inputId.includes("capa")) photoType = "capa";
          else if (inputId.includes("perfil") || inputId.includes("stories"))
            photoType = "stories";

          const maxSize = limits[photoType];
          const maxSizeMB = (maxSize / (1024 * 1024)).toFixed(1);

          // Validar tamanho do arquivo
          if (file.size > maxSize) {
            alert(
              `O arquivo √© muito grande (${(file.size / (1024 * 1024)).toFixed(
                1
              )} MB). Por favor, selecione uma imagem menor que ${maxSizeMB} MB.`
            );
            input.value = ""; // Limpar o input
            return;
          }

          // Fun√ß√£o de compress√£o inteligente
          function compressImage(
            file,
            maxWidth = 1200,
            maxHeight = 1600,
            quality = 0.85
          ) {
            return new Promise((resolve) => {
              const canvas = document.createElement("canvas");
              const ctx = canvas.getContext("2d");
              const img = new Image();

              img.onload = function () {
                // Calcular novas dimens√µes mantendo propor√ß√£o
                let { width, height } = img;

                if (width > maxWidth || height > maxHeight) {
                  const ratio = Math.min(maxWidth / width, maxHeight / height);
                  width *= ratio;
                  height *= ratio;
                }

                // Configurar canvas
                canvas.width = width;
                canvas.height = height;

                // Desenhar imagem redimensionada
                ctx.drawImage(img, 0, 0, width, height);

                // Converter para blob com qualidade otimizada
                canvas.toBlob(resolve, "image/jpeg", quality);
              };

              img.src = URL.createObjectURL(file);
            });
          }

          // Processar imagem com compress√£o
          compressImage(file).then((compressedFile) => {
            const reader = new FileReader();
            reader.onload = function (e) {
              // Criar imagem para verificar dimens√µes
              const img = new Image();
              img.onload = function () {
                // Adicionar a imagem ao preview
                preview.innerHTML = `<img src="${e.target.result}" alt="Preview" style="max-width: 100%; max-height: 100%; object-fit: cover;">`;
                preview.classList.add("show");
                preview.style.display = "block";

                // Atualizar texto do upload com informa√ß√µes de compress√£o
                const originalSize = (file.size / 1024).toFixed(0);
                const compressedSize = (compressedFile.size / 1024).toFixed(0);
                uploadText.innerHTML = `
                    ${file.name}<br>
                    <small style="color: #666;">Original: ${originalSize}KB ‚Üí Otimizada: ${compressedSize}KB</small>
                  `;
                uploadText.style.color = "#28a745";

                // Adicionar classe para indicar que tem foto
                uploadBox.classList.add("has-photo");
              };
              img.src = e.target.result;
            };
            reader.readAsDataURL(compressedFile);
          });
        }

        // Event listener para mudan√ßa no input
        input.addEventListener("change", function (e) {
          console.log("üìÅ Arquivo selecionado:", e.target.files.length);
          const file = e.target.files[0];
          if (file) {
            processFile(file);
          }
        });

        // Adicionar event listener adicional para garantir que funcione
        input.addEventListener("click", function (e) {
          console.log("üñ±Ô∏è Input clicado diretamente");
          e.stopPropagation();
        });

        // Event listener para clique na caixa de upload
        uploadBox.addEventListener("click", function (e) {
          // N√£o processar se o clique foi no pr√≥prio input ou em elementos filhos do input
          if (e.target === input || input.contains(e.target)) {
            return;
          }

          // N√£o processar se o clique foi na imagem de preview
          if (
            e.target.tagName === "IMG" ||
            e.target.closest(".photo-preview")
          ) {
            return;
          }

          e.preventDefault();
          e.stopPropagation();

          // Limpar o input antes de abrir o seletor
          input.value = "";

          // Usar setTimeout para garantir que o input seja limpo antes de abrir
          setTimeout(() => {
            input.click();
          }, 10);
        });
      }

      // Configurar uploads para todas as fotos
      setupPhotoUpload("capa-photo", "capa-preview", "capa-upload-box");
      setupPhotoUpload("perfil-photo", "perfil-preview", "perfil-upload-box");

      // Configurar uploads para galeria (8 fotos)
      for (let i = 1; i <= 8; i++) {
        setupPhotoUpload(
          `galeria-photo-${i}`,
          `galeria-preview-${i}`,
          `galeria-upload-box-${i}`
        );
      }

      // JavaScript para upload de v√≠deo com valida√ß√£o de dura√ß√£o
      function setupVideoUpload(inputId, previewId, uploadBoxId) {
        const input = document.getElementById(inputId);
        const preview = document.getElementById(previewId);
        const uploadBox = document.getElementById(uploadBoxId);

        if (!input || !preview || !uploadBox) {
          console.warn(`Elementos n√£o encontrados para ${inputId}`);
          return;
        }

        input.addEventListener('change', function(e) {
          const file = e.target.files[0];
          if (!file) return;

          // Validar tipo de arquivo
          if (!file.type.startsWith('video/')) {
            alert('Por favor, selecione apenas arquivos de v√≠deo.');
            input.value = '';
            return;
          }

          // Validar tamanho do arquivo (100MB m√°ximo)
          const maxSize = 100 * 1024 * 1024; // 100MB
          if (file.size > maxSize) {
            alert(`O v√≠deo √© muito grande (${(file.size / (1024 * 1024)).toFixed(1)} MB). M√°ximo permitido: 100MB.`);
            input.value = '';
            return;
          }

          // Validar dura√ß√£o do v√≠deo (30 segundos m√°ximo)
          const video = document.createElement('video');
          video.preload = 'metadata';
          
          video.onloadedmetadata = function() {
            window.URL.revokeObjectURL(video.src);
            
            const duration = video.duration;
            const maxDuration = 30; // 30 segundos
            
            if (duration > maxDuration) {
              alert(`O v√≠deo √© muito longo (${duration.toFixed(1)} segundos). M√°ximo permitido: ${maxDuration} segundos.`);
              input.value = '';
              return;
            }

            // Se passou em todas as valida√ß√µes, mostrar preview
            showVideoPreview(file, preview);
          };

          video.onerror = function() {
            alert('Erro ao carregar o v√≠deo. Verifique se o arquivo √© v√°lido.');
            input.value = '';
          };

          video.src = URL.createObjectURL(file);
        });
      }

      // Fun√ß√£o para mostrar preview do v√≠deo
      function showVideoPreview(file, previewElement) {
        const video = document.createElement('video');
        video.src = URL.createObjectURL(file);
        video.controls = true;
        video.style.width = '100%';
        video.style.maxWidth = '300px';
        video.style.borderRadius = '8px';
        video.style.marginTop = '10px';

        // Adicionar informa√ß√µes do arquivo
        const info = document.createElement('div');
        info.style.marginTop = '5px';
        info.style.fontSize = '12px';
        info.style.color = '#666';
        info.innerHTML = `
          <strong>${file.name}</strong><br>
          Tamanho: ${(file.size / (1024 * 1024)).toFixed(1)} MB
        `;

        // Limpar preview anterior
        previewElement.innerHTML = '';
        
        // Adicionar novo preview
        previewElement.appendChild(video);
        previewElement.appendChild(info);
      }

      // Configurar upload de v√≠deo
      setupVideoUpload("videoCapa", "video-preview", "video-upload-box");

      // JavaScript para a se√ß√£o PERFIL
      // Radio buttons da se√ß√£o PERFIL
      const perfilRadioGroups = [
