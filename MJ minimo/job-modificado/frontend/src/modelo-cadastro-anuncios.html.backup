          // Esconde o popup
          changePopup.style.display = 'none';
          pendingChange = null;
          
          console.log("‚ùå Mudan√ßa cancelada via popup - voltando √† escolha anterior");
        });

        // Debug: Verificar se os elementos foram encontrados
        console.log("üîç Debug - Elementos encontrados:");
        console.log("- genderRadios:", genderRadios.length);
        console.log("- categoryRadios:", categoryRadios.length);
        console.log("- confirmationSection:", confirmationSection);
        console.log("- selectedChoiceText:", selectedChoiceText);
        console.log("- btnConfirm:", btnConfirm);
        console.log("- btnChange:", btnChange);
        
        // Verificar se os elementos existem
        if (!confirmationSection) {
          console.error("‚ùå ERRO: confirmationSection n√£o encontrado!");
        }
        if (!selectedChoiceText) {
          console.error("‚ùå ERRO: selectedChoiceText n√£o encontrado!");
        }
        if (genderRadios.length === 0) {
          console.error("‚ùå ERRO: Nenhum gender radio encontrado!");
        }
        if (categoryRadios.length === 0) {
          console.error("‚ùå ERRO: Nenhum category radio encontrado!");
        }
        
        // Teste manual removido - agora vamos testar os event listeners

        // =====================================
        // VALIDA√á√ÉO DE IDADE
        // =====================================
        const idadeInput = document.getElementById("idade");
        if (idadeInput) {
          idadeInput.addEventListener("input", (e) => {
            let value = e.target.value;
            
            // Remove caracteres n√£o num√©ricos
            value = value.replace(/\D/g, '');
            
            // Converte para n√∫mero
            const numValue = parseInt(value);
            
            // Valida√ß√£o: 18-89 anos
            if (value && (numValue < 18 || numValue > 89)) {
              // Bloqueia a digita√ß√£o se for menor que 18 ou maior que 89
              if (numValue < 18) {
                e.target.value = '18';
                e.target.style.borderColor = '#dc3545';
                e.target.style.backgroundColor = '#ffeaea';
                showAgeError("Idade m√≠nima: 18 anos");
              } else if (numValue > 89) {
                e.target.value = '89';
                e.target.style.borderColor = '#dc3545';
                e.target.style.backgroundColor = '#ffeaea';
                showAgeError("Idade m√°xima: 89 anos");
              }
            } else {
              e.target.value = value;
              e.target.style.borderColor = '';
              e.target.style.backgroundColor = '';
              hideAgeError();
            }
          });

          // Fun√ß√£o para mostrar erro de idade
          function showAgeError(message) {
            let errorDiv = document.getElementById("idade-error");
            if (!errorDiv) {
              errorDiv = document.createElement("div");
              errorDiv.id = "idade-error";
              errorDiv.style.cssText = `
                color: #dc3545;
                font-size: 12px;
                margin-top: 5px;
                font-weight: bold;
              `;
              idadeInput.parentNode.appendChild(errorDiv);
            }
            errorDiv.textContent = message;
          }

          // Fun√ß√£o para esconder erro de idade
          function hideAgeError() {
            const errorDiv = document.getElementById("idade-error");
            if (errorDiv) {
              errorDiv.remove();
            }
          }
