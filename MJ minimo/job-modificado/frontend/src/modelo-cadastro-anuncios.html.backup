              // Desbloqueia as se√ß√µes espec√≠ficas
              unlockSpecificSections(pendingChange.gender, pendingChange.category);
              
              // Esconde o popup
              changePopup.style.display = 'none';
              
              // For√ßa outro delay antes de bloquear os bot√µes
              setTimeout(() => {
                // Mant√©m os bot√µes bloqueados
                blockChoiceButtons();
                
                isConfirmed = true;
                pendingChange = null;
                
                console.log("‚úÖ Mudan√ßa confirmada via popup:", currentChoice);
                console.log("‚úÖ Apenas as novas op√ß√µes est√£o marcadas e bloqueadas");
              }, 100);
              
            }, 100);
          }
        });

        popupCancelChange.addEventListener("click", () => {
          // Volta para a escolha anterior
          if (currentChoice) {
            const genderInput = document.querySelector(`input[name="category"][value="${currentChoice.gender}"]`);
            const categoryInput = document.querySelector(`input[name="categoria"][value="${currentChoice.category}"]`);
            
            if (genderInput && categoryInput) {
              genderInput.checked = true;
              categoryInput.checked = true;
            }
