      /* Bot√µes bloqueados */
      .btn-blocked {
        pointer-events: none;
        opacity: 0.5;
        filter: grayscale(100%);
        cursor: not-allowed !important;
        position: relative;
      }

      .btn-blocked::after {
        content: "üîí";
        position: absolute;
        top: 50%;
        left: 50%;
