    <script>
      // Prevenir submiss√£o acidental por Enter
      document.addEventListener("DOMContentLoaded", function () {
        const form = document.getElementById("cadastroForm");

        // Prevenir Enter em campos de texto (exceto textarea)
        form.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            const target = e.target;

            // Permitir Enter apenas em textarea
            if (target.tagName === "TEXTAREA") {
              return; // Permite Enter em textarea
            }

            // Prevenir Enter em outros campos
            e.preventDefault();

            // Se for um campo de input, focar no pr√≥ximo campo
            if (target.tagName === "INPUT") {
              const inputs = Array.from(
                form.querySelectorAll("input, select, textarea")
              );
              const currentIndex = inputs.indexOf(target);
              const nextInput = inputs[currentIndex + 1];

              if (nextInput) {
                nextInput.focus();
              }
            }
          }
        });

        // Adicionar confirma√ß√£o antes de submeter
        form.addEventListener("submit", function (e) {
          e.preventDefault();

          // Verificar se todos os campos obrigat√≥rios est√£o preenchidos
          const requiredFields = form.querySelectorAll("[required]");
          let allFieldsValid = true;
          let missingFields = [];

          requiredFields.forEach((field) => {
            if (!field.value.trim()) {
              allFieldsValid = false;
              const label =
                field.previousElementSibling?.textContent || field.name;
              missingFields.push(label);
            }
          });

          if (!allFieldsValid) {
            alert(
              "Por favor, preencha todos os campos obrigat√≥rios antes de finalizar o cadastro."
            );
            return;
          }

          // Confirma√ß√£o final
          const confirmMessage =
            `Tem certeza que deseja finalizar o cadastro?\n\n` +
            `Esta a√ß√£o n√£o pode ser desfeita.\n` +
            `Clique em "OK" para confirmar ou "Cancelar" para continuar editando.`;

          if (confirm(confirmMessage)) {
            // Se confirmado, chamar a fun√ß√£o de submiss√£o confirmada
            if (typeof submitFormConfirmed === "function") {
              submitFormConfirmed();
            } else {
              // Fallback: submeter normalmente
              form.submit();
            }
          }
        });

        console.log("üõ°Ô∏è Prote√ß√£o contra submiss√£o acidental ativada!");

        // Adicionar indicador visual de prote√ß√£o
        const submitButton = form.querySelector(".btn-submit");
        if (submitButton) {
          submitButton.title =
            "Clique para finalizar o cadastro (protegido contra Enter acidental)";
        }

        // Fun√ß√£o para processar submiss√£o confirmada
        window.submitFormConfirmed = function() {
          console.log("üöÄ Processando submiss√£o do formul√°rio...");
          
          // Mostrar loading
          const submitBtn = document.querySelector(".btn-submit");
          const originalText = submitBtn.innerHTML;
          submitBtn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Criando an√∫ncio...';
          submitBtn.disabled = true;
          
          // Simular processamento (substitua por sua l√≥gica real)
          setTimeout(() => {
            // Aqui voc√™ pode adicionar sua l√≥gica de envio do formul√°rio
            // Por exemplo, enviar para uma API ou processar os dados
            
            // Mostrar mensagem de sucesso
            alert("‚úÖ An√∫ncio criado com sucesso! Redirecionando...");
            
            // Redirecionar para a p√°gina modificada
            window.location.href = "anunciar_GP_02_modificado.html";
          }, 2000);
        };

        // üéØ CAPITALIZAR PRIMEIRA LETRA DO CAMPO NOME
        const nomeField = document.getElementById("nome");
        if (nomeField) {
          // Fun√ß√£o para capitalizar primeira letra
          function capitalizeFirstLetter(text) {
            if (!text) return text;
            return text.charAt(0).toUpperCase() + text.slice(1).toLowerCase();
          }

          // Event listener para capitalizar em tempo real
          nomeField.addEventListener("input", function() {
            const cursorPosition = this.selectionStart;
            const originalValue = this.value;
            const capitalizedValue = capitalizeFirstLetter(originalValue);
            
            // S√≥ atualizar se houve mudan√ßa
            if (originalValue !== capitalizedValue) {
              this.value = capitalizedValue;
              // Manter posi√ß√£o do cursor
              this.setSelectionRange(cursorPosition, cursorPosition);
            }
          });

          // Event listener para capitalizar ao sair do campo (blur)
          nomeField.addEventListener("blur", function() {
            this.value = capitalizeFirstLetter(this.value);
          });

          console.log("‚úÖ Capitaliza√ß√£o autom√°tica configurada para o campo 'nome'");
        }
      });
    </script>
    
    <!-- Sistema de Otimiza√ß√£o de Imagens -->
    <script src="js/image-optimizer.js"></script>
    <script>
      // Integrar sistema de otimiza√ß√£o com o formul√°rio existente
      document.addEventListener('DOMContentLoaded', function() {
        if (imageOptimizer) {
          // Interceptar envio do formul√°rio para usar imagem otimizada
          const form = document.querySelector('form');
          if (form) {
            form.addEventListener('submit', function(e) {
              const imageData = imageOptimizer.getImageData();
              if (imageData) {
                console.log('üöÄ Usando imagem otimizada:', imageData);
                
                // Adicionar dados da imagem otimizada ao FormData
                const formData = new FormData(form);
                
                // Remover arquivos duplicados e usar apenas a imagem principal
                formData.delete('foto_capa');
                formData.delete('foto_stories');
                formData.delete('banner');
                
                // Adicionar imagem principal para todos os usos
                formData.append('foto_capa', imageData.file);
                formData.append('foto_stories', imageData.file);
                formData.append('banner', imageData.file);
                
                // Adicionar flag de otimiza√ß√£o
                formData.append('image_optimized', 'true');
                formData.append('optimization_usage', JSON.stringify(imageData.usage));
                
                console.log('‚úÖ FormData otimizado preparado');
              }
            });
          }
        }
      });

    </script>

    <script>
      // =====================================
      // BLOQUEIO IMEDIATO DAS SE√á√ïES
      // =====================================
      // Executa imediatamente, antes do DOM estar pronto
      (function() {
        console.log("üöÄ BLOQUEIO IMEDIATO - Executando antes do DOM...");
        
        // Fun√ß√£o para bloquear se√ß√µes
        function blockSectionsImmediately() {
          const sections = document.querySelectorAll('.form-section');
          sections.forEach(sec => {
            if (sec.id !== 'step-1' && sec.id !== 'step-2') {
              sec.style.display = 'none';
              sec.style.visibility = 'hidden';
              sec.style.opacity = '0';
              sec.style.height = '0';
              sec.style.overflow = 'hidden';
              console.log(`üîí BLOQUEIO IMEDIATO: ${sec.id}`);
            }
          });
        }
        
        // Executa imediatamente
        blockSectionsImmediately();
        
        // Executa quando o DOM estiver pronto
        if (document.readyState === 'loading') {
          document.addEventListener('DOMContentLoaded', blockSectionsImmediately);
        } else {
          blockSectionsImmediately();
        }
        
        // Executa ap√≥s um pequeno delay para garantir
        setTimeout(blockSectionsImmediately, 100);
        setTimeout(blockSectionsImmediately, 500);
      })();

