        // Event listeners para mudan√ßas nas escolhas
        function onChoiceChange() {
          console.log("üîÑ onChoiceChange chamada!");
          
          const gender = document.querySelector('input[name="category"]:checked')?.value;
          const category = document.querySelector('input[name="categoria"]:checked')?.value;

          console.log("üîÑ Escolha alterada:", { gender, category });
          console.log("üîÑ Estado atual:", { isConfirmed, currentChoice });

          if (gender && category) {
            const newChoice = { gender, category };
            
