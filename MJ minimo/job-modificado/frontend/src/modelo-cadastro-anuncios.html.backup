        blockAllSections();
        
        console.log("‚úÖ Inicializa√ß√£o conclu√≠da - apenas se√ß√µes 1 e 2 vis√≠veis");

        // =====================================
        // BOT√ïES DE DEBUG REMOVIDOS
        // =====================================
        // Bot√µes de debug removidos para n√£o atrapalhar a funcionalidade
      });

      // =====================================
      // SCRIPT DE REDIRECIONAMENTO AUTOM√ÅTICO
      // =====================================
      document.addEventListener("DOMContentLoaded", () => {
        const form = document.getElementById("cadastroForm");

        form.addEventListener("submit", (e) => {
          e.preventDefault(); // evita recarregar a p√°gina

          // L√™ os valores selecionados
          const gender = document.querySelector('input[name="category"]:checked')?.value;
          const category = document.querySelector('input[name="categoria"]:checked')?.value;

          if (!gender || !category) {
            alert("Por favor, selecione as op√ß√µes 'Voc√™ √©' e 'Minha categoria'.");
            return;
          }

          // Armazena temporariamente as escolhas (para outras p√°ginas poderem ler)
          localStorage.setItem("userGender", gender);
          localStorage.setItem("userCategory", category);

          // Redirecionamento baseado nas combina√ß√µes
          let redirectUrl = "";

          // =============== ACOMPANHANTES ===============
          if (category === "acompanhantes") {
            if (gender === "mulher") redirectUrl = "A_02__premium.html";
            else if (gender === "trans") redirectUrl = "A_04__trans.html";
            else if (gender === "homem") redirectUrl = "A_05__homens.html";
            else if (gender === "mulher-luxo") redirectUrl = "luxo.html";
          }

          // =============== MASSAGISTAS ===============
          if (category === "massagistas") {
            if (gender === "mulher") redirectUrl = "A_03__massagistas.html";
            else if (gender === "mulher-luxo") redirectUrl = "luxo.html";
            else if (gender === "trans") redirectUrl = "A_04__trans.html";
            else if (gender === "homem") redirectUrl = "A_05__homens.html";
          }

          // =============== SEXO VIRTUAL ===============
          if (category === "sexo-virtual") {
            if (gender === "mulher") redirectUrl = "A_02__premium_copy.html";
            else if (gender === "trans") redirectUrl = "A_04__trans.html";
            else if (gender === "homem") redirectUrl = "A_05__homens.html";
            else if (gender === "mulher-luxo") redirectUrl = "luxo.html";
          }

          if (redirectUrl === "") {
            alert("N√£o foi poss√≠vel identificar a p√°gina correta. Verifique suas sele√ß√µes.");
            return;
          }

          // Exibe feedback e redireciona
          alert("Cadastro conclu√≠do! Voc√™ ser√° redirecionado para sua p√°gina de an√∫ncios.");
          window.location.href = redirectUrl;
        });
      });
    </script>

