      // Verificar autentica√ß√£o e carregar dados do usu√°rio
      let currentUser = null;
      let userData = null;

      // Verificar se o usu√°rio est√° autenticado
      auth.onAuthStateChanged(async (user) => {
        if (user) {
          currentUser = user;
          console.log("Usu√°rio autenticado:", user.email);

          // Carregar dados do usu√°rio
          await loadUserData(user.uid);
        } else {
          // Usu√°rio n√£o autenticado, redirecionar para login
          console.log("Usu√°rio n√£o autenticado, redirecionando...");
          window.location.href = "criar-conta-Anuncio.html";
        }
      });

      // Carregar dados do usu√°rio do Firebase
      async function loadUserData(uid) {
        try {
          const doc = await db.collection("advertisers").doc(uid).get();
          if (doc.exists) {
            userData = doc.data();
            console.log("Dados do usu√°rio carregados:", userData);

            // Preencher campos do formul√°rio com dados existentes
            fillFormWithUserData(userData);
          } else {
            console.log("Nenhum dado encontrado para o usu√°rio");
            userData = {
              email: currentUser.email,
              role: "advertiser",
              status: "active", // ‚úÖ Usu√°rios criados diretamente como ativos
            };
          }
        } catch (error) {
          console.error("Erro ao carregar dados do usu√°rio:", error);
          alert("Erro ao carregar dados. Tente novamente.");
        }
      }

      // Preencher formul√°rio com dados do usu√°rio
      function fillFormWithUserData(data) {
        // Preencher campos b√°sicos se existirem
        if (data.nome) {
          const nomeField = document.getElementById("nome");
          if (nomeField) nomeField.value = data.nome || "";
        }
        if (data.email) {
          const emailField = document.getElementById("email");
          if (emailField) emailField.value = data.email || "";
        }
        if (data.telefone) {
          const telefoneField = document.getElementById("telefone");
          if (telefoneField) telefoneField.value = data.telefone || "";
        }
        if (data.telefoneFixo) {
          const telefoneFixoField = document.getElementById("telefoneFixo");
          if (telefoneFixoField)
            telefoneFixoField.value = data.telefoneFixo || "";
        }
        if (data.idade) {
          const idadeField = document.getElementById("idade");
          if (idadeField) idadeField.value = data.idade || "";
        }
        if (data.altura) {
          const alturaField = document.getElementById("altura");
          if (alturaField) alturaField.value = data.altura || "";
        }
        if (data.peso) {
          const pesoField = document.getElementById("peso");
          if (pesoField) pesoField.value = data.peso || "";
        }
        if (data.descricao) {
          const descricaoField = document.getElementById("descricao");
          if (descricaoField) descricaoField.value = data.descricao || "";
        }

        // Preencher sele√ß√µes
        if (data.genero) {
          const generoRadio = document.querySelector(
            `input[name="category"][value="${data.genero}"]`
          );
          if (generoRadio) generoRadio.checked = true;
        }

        if (data.categoria) {
          const categoriaRadio = document.querySelector(
            `input[name="categoria"][value="${data.categoria}"]`
          );
          if (categoriaRadio) categoriaRadio.checked = true;
        }
      }

      // Vari√°veis globais para armazenar as inst√¢ncias do intl-tel-input
      let itiCelular;
      let itiFixo;

      // Fun√ß√£o para formatar telefone brasileiro visualmente
      function formatarTelefoneBrasileiro(input) {
        let valor = input.value.replace(/\D/g, ""); // Remove tudo que n√£o √© d√≠gito

        if (valor.length <= 2) {
          input.value = valor;
        } else if (valor.length <= 6) {
          input.value = `(${valor.slice(0, 2)}) ${valor.slice(2)}`;
        } else if (valor.length <= 10) {
          input.value = `(${valor.slice(0, 2)}) ${valor.slice(
            2,
            6
          )}-${valor.slice(6)}`;
        } else if (valor.length <= 11) {
          // Para celular com 11 d√≠gitos (incluindo o 9)
          input.value = `(${valor.slice(0, 2)}) ${valor.slice(
            2,
            7
          )}-${valor.slice(7)}`;
        } else {
          // Limita a 11 d√≠gitos para telefone brasileiro
          valor = valor.slice(0, 11);
          input.value = `(${valor.slice(0, 2)}) ${valor.slice(
            2,
            7
          )}-${valor.slice(7)}`;
        }
      }

      // Fun√ß√£o para obter o n√∫mero de telefone celular formatado
      function obterTelefoneCelularCompleto() {
        if (itiCelular) {
          const numero = itiCelular.getNumber();
          const codigoPais = itiCelular.getSelectedCountryData().dialCode;
          const nomePais = itiCelular.getSelectedCountryData().name;

          console.log("N√∫mero celular completo:", numero);
          console.log("C√≥digo do pa√≠s:", codigoPais);
          console.log("Nome do pa√≠s:", nomePais);

          return {
            numero: numero,
            codigoPais: codigoPais,
            nomePais: nomePais,
            numeroLimpo: numero.replace(/[^\d+]/g, ""),
          };
        }
        return null;
      }

      // Fun√ß√£o para obter o n√∫mero de telefone fixo formatado
      function obterTelefoneFixoCompleto() {
        if (itiFixo) {
          const numero = itiFixo.getNumber();
          const codigoPais = itiFixo.getSelectedCountryData().dialCode;
          const nomePais = itiFixo.getSelectedCountryData().name;

          console.log("N√∫mero fixo completo:", numero);
          console.log("C√≥digo do pa√≠s:", codigoPais);
          console.log("Nome do pa√≠s:", nomePais);

          return {
            numero: numero,
            codigoPais: codigoPais,
            nomePais: nomePais,
            numeroLimpo: numero.replace(/[^\d+]/g, ""),
          };
        }
        return null;
      }

      // Fun√ß√£o para validar o n√∫mero de telefone celular
      function validarTelefoneCelular() {
        if (itiCelular) {
          const isValid = itiCelular.isValidNumber();
          const input = document.getElementById("telefone_celular");
          const itiContainer = input.closest(".iti");

          // Remover classes de valida√ß√£o anteriores
          itiContainer.classList.remove("iti--valid", "iti--invalid");

          if (isValid) {
            input.setCustomValidity("");
            itiContainer.classList.add("iti--valid");
          } else {
            input.setCustomValidity(
              "Por favor, insira um n√∫mero de celular/WhatsApp v√°lido"
            );
            itiContainer.classList.add("iti--invalid");
          }

          return isValid;
        }
        return false;
      }

      // Fun√ß√£o para validar o n√∫mero de telefone fixo
      function validarTelefoneFixo() {
        if (itiFixo) {
          const isValid = itiFixo.isValidNumber();
          const input = document.getElementById("telefone_fixo");
          const itiContainer = input.closest(".iti");

          // Remover classes de valida√ß√£o anteriores
          itiContainer.classList.remove("iti--valid", "iti--invalid");

          if (isValid) {
            input.setCustomValidity("");
            itiContainer.classList.add("iti--valid");
          } else {
            input.setCustomValidity(
              "Por favor, insira um n√∫mero de telefone fixo v√°lido"
            );
            itiContainer.classList.add("iti--invalid");
          }

          return isValid;
        }
        return true; // Telefone fixo √© opcional, ent√£o retorna true se n√£o estiver preenchido
      }

      // Fun√ß√£o para obter o n√∫mero limpo do celular (sem formata√ß√£o) para envio ao banco
      function obterNumeroCelularLimpo() {
        if (itiCelular) {
          const numero = itiCelular.getNumber();
          // Remove todos os caracteres n√£o num√©ricos exceto o +
          return numero.replace(/[^\d+]/g, "");
        }
        return "";
      }

      // Fun√ß√£o para obter o n√∫mero limpo do fixo (sem formata√ß√£o) para envio ao banco
      function obterNumeroFixoLimpo() {
        if (itiFixo) {
          const numero = itiFixo.getNumber();
          // Remove todos os caracteres n√£o num√©ricos exceto o +
          return numero.replace(/[^\d+]/g, "");
        }
        return "";
      }

      // Script para carregar estados e cidades usando API do IBGE
      document.addEventListener("DOMContentLoaded", function () {
        console.log("üöÄ DOMContentLoaded executado!");

        // Configurar valida√ß√£o de descri√ß√£o
        const descricaoTextarea = document.getElementById("descricao");
        const charCount = document.getElementById("charCount");
        const charWarning = document.getElementById("charWarning");
        const charSuccess = document.getElementById("charSuccess");
        const charsNeeded = document.getElementById("charsNeeded");

        const MIN_CHARS = 250;
        const MAX_CHARS = 500;

        function validarDescricao() {
          let texto = descricaoTextarea.value;

          // N√ÉO modificar o texto durante a digita√ß√£o - apenas validar
          // Permitir espa√ßos normais entre palavras
          // Apenas remover espa√ßos excessivos no final para valida√ß√£o
          let textoParaValidacao = texto.trim();

          // Para valida√ß√£o, remover apenas 4 ou mais espa√ßos consecutivos (para evitar spam)
          textoParaValidacao = textoParaValidacao.replace(/\s{4,}/g, "   ");

          // Remove quebras de linha m√∫ltiplas e substitui por uma √∫nica
          textoParaValidacao = textoParaValidacao.replace(/\n\s*\n/g, "\n");

          // Detectar tentativas de burla com caracteres repetidos
          // Remove sequ√™ncias de 3 ou mais caracteres iguais consecutivos
          textoParaValidacao = textoParaValidacao.replace(/(.)\1{2,}/g, "$1$1");

          // Remove sequ√™ncias de pontos, v√≠rgulas ou outros caracteres especiais repetidos
          textoParaValidacao = textoParaValidacao.replace(
            /[.,;:!?]{3,}/g,
            "..."
          );

          // Verificar se o texto tem conte√∫do real (n√£o apenas espa√ßos e caracteres especiais)
          const textoLimpo = textoParaValidacao
            .replace(/[^\w\s]/g, "")
            .replace(/\s{3,}/g, " ")
            .trim();
          const temConteudoReal = textoLimpo.length >= MIN_CHARS * 0.5; // Pelo menos 50% deve ser conte√∫do real

          // Contar caracteres do texto original (sem modificar)
          const length = texto.length;

          // Atualizar contador
          charCount.textContent = length;

          // Remover classes anteriores
          charCount.classList.remove("warning", "success");
          descricaoTextarea.classList.remove("warning", "success");
          charWarning.style.display = "none";
          charSuccess.style.display = "none";

          if (length < MIN_CHARS || !temConteudoReal) {
            // Mostrar aviso
            const needed = MIN_CHARS - length;
            charsNeeded.textContent = needed;
            charCount.classList.add("warning");
            descricaoTextarea.classList.add("warning");
            charWarning.style.display = "block";

            // Definir valida√ß√£o customizada
            if (!temConteudoReal) {
              descricaoTextarea.setCustomValidity(
                `A descri√ß√£o deve ter conte√∫do real, n√£o apenas espa√ßos ou caracteres especiais. Escreva pelo menos ${Math.ceil(
                  MIN_CHARS * 0.7
                )} caracteres de texto v√°lido.`
              );
            } else {
              descricaoTextarea.setCustomValidity(
                `Voc√™ precisa de mais ${needed} caracteres para atingir o m√≠nimo de ${MIN_CHARS} caracteres.`
              );
            }
          } else if (length > MAX_CHARS) {
            // Mostrar aviso de limite excedido
            charCount.classList.add("warning");
            descricaoTextarea.classList.add("warning");
            charWarning.textContent = `Voc√™ excedeu o limite de ${MAX_CHARS} caracteres.`;
            charWarning.style.display = "block";

            descricaoTextarea.setCustomValidity(
              `Voc√™ excedeu o limite de ${MAX_CHARS} caracteres.`
            );
          } else {
            // Mostrar sucesso
            charCount.classList.add("success");
            descricaoTextarea.classList.add("success");
            charSuccess.style.display = "block";

            descricaoTextarea.setCustomValidity("");
          }
        }

        if (descricaoTextarea) {
          // Adicionar eventos
          descricaoTextarea.addEventListener("input", validarDescricao);
          descricaoTextarea.addEventListener("blur", validarDescricao);
          descricaoTextarea.addEventListener("focus", function () {
            validarDescricao();
          });

          // Validar inicialmente
          validarDescricao();

          console.log("üìù Valida√ß√£o de descri√ß√£o configurada com sucesso!");
        }

        // Configurar intl-tel-input para telefone celular
        const inputCelular = document.querySelector("#telefone_celular");
        if (inputCelular) {
          itiCelular = window.intlTelInput(inputCelular, {
            initialCountry: "br", // Brasil como pa√≠s inicial
            preferredCountries: [
              "br",
              "us",
              "ar",
              "cl",
              "co",
              "pe",
              "uy",
              "py",
              "bo",
              "ec",
              "ve",
            ], // Pa√≠ses preferenciais da Am√©rica Latina
            separateDialCode: true, // Mostra o c√≥digo do pa√≠s separado
            utilsScript:
              "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.19/js/utils.js", // Para valida√ß√£o
            autoPlaceholder: "aggressive", // Placeholder autom√°tico baseado no pa√≠s
            nationalMode: false, // Permite entrada internacional
            formatOnDisplay: true, // Formata o n√∫mero na exibi√ß√£o
            geoIpLookup: function (callback) {
              // Usar Brasil como padr√£o se n√£o conseguir detectar
              callback("br");
            },
          });

          // Adicionar eventos para valida√ß√£o, foco e formata√ß√£o do celular
          inputCelular.addEventListener("blur", function () {
            validarTelefoneCelular();
            inputCelular.closest(".iti").classList.remove("iti--focused");
          });

          inputCelular.addEventListener("focus", function () {
            inputCelular.closest(".iti").classList.add("iti--focused");
          });

          inputCelular.addEventListener("input", function () {
            // Aplicar formata√ß√£o visual apenas para telefones brasileiros
            const codigoPais = itiCelular.getSelectedCountryData().dialCode;
            if (codigoPais === "55") {
              // Brasil
              formatarTelefoneBrasileiro(inputCelular);
            }
          });

          inputCelular.addEventListener("countrychange", function () {
            console.log(
              "Pa√≠s celular alterado para:",
              itiCelular.getSelectedCountryData().name
            );
